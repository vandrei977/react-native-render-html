    "commitlint": "^12.1.1",
    "conditional-type-checks": "^1.0.5",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "inkscape": "^3.1.1",
    "metro-react-native-babel-preset": "^0.66.0",
